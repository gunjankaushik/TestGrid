pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'This is Build Stage - Stage 1'
            }
        }
    
      
        stage('Test') {
            steps {
                echo 'This is Test Stage - Stage 2'
            }
        }
    
     
      
        stage('Release') {
            steps {
                echo 'This is Release Stage - Stage 3'
            }
        }
    }
}
